        label="rule: program"];
    2    [color=darkgray,
        label="rule: stmt"];
    1 -> 2;
    11    [label=";"];
    1 -> 11;
    12    [label=<EOF>];
    1 -> 12;
    3    [color=darkgray,
        label="rule: bind"];
    2 -> 3;
    4    [label=let];
    3 -> 4;
    5    [color=darkgray,
        label="rule: pattern"];
    3 -> 5;
    7    [label="="];
    3 -> 7;
    8    [color=darkgray,
        label="rule: expr"];
    3 -> 8;
    6    [label=a];
    5 -> 6;
    9    [color=darkgray,
        label="rule: val"];
    8 -> 9;
    10    [label=1];
    9 -> 10;
}
